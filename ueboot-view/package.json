{
  "name": "ueboot",
  "version": "1.7.45",
  "description": "ueboot是一个基于`spring boot2 + vue2 + iview3`的基础开发平台，通过这个平台可以快速完成某些CRUD功能的系统（从前端到后端快速实现）",
  "author": "ueboot",
  "scripts": {
    "dev": "webpack-dev-server --content-base test/  --inline --hot --compress --history-api-fallback --port 9999 --config build/webpack.dev.config.js",
    "dev:s": "webpack-dev-server --content-base test/ --open --inline --hot --compress --history-api-fallback --port 8081 --host 0.0.0.0 --config build/webpack.dev.config.js",
    "dist:style": "gulp --gulpfile build/build-style.js",
    "dist:dev": "webpack --config build/webpack.dist.dev.config.js",
    "dist:prod": "webpack --config build/webpack.dist.prod.config.js",
    "dist:locale": "webpack --config build/webpack.dist.locale.config.js",
    "dist": "npm run dist:style && npm run dist:dev && npm run dist:prod",
    "lint": "eslint --fix --ext .js,.vue src",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "test": "npm run lint && npm run unit",
    "prepare": "npm run dist"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "deep-extend": "^0.6.0",
    "font-awesome": "^4.7.0",
    "iview": "^3.2.2",
    "less": "^3.9.0",
    "moment": "^2.24.0",
    "vue": "^2.6.6",
    "vue-color": "^2.7.0",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.1",
    "@vue/cli-plugin-eslint": "^3.5.1",
    "@vue/cli-service": "^3.4.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "less-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/standard"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "bugs": {
    "url": "https://github.com/ueboot/ueboot/issues"
  },
  "files": [
    "dist",
    "src"
  ],
  "homepage": "http://www.ueboot.com",
  "keywords": [
    "iview",
    "vue",
    "vue.js",
    "component",
    "components",
    "ui",
    "framework",
    "ueboot"
  ],
  "license": "MIT",
  "main": "dist/ueboot.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ueboot/ueboot.git"
  }
}
